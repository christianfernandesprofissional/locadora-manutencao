<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="560.0" prefWidth="581.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fatec.garagemlocalhost.ManutencaoFinalizadaController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label text="SERVICOS REALIZADOS" VBox.vgrow="ALWAYS">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TableView fx:id="tabelaListaServicos" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="colunaIdServico" prefWidth="75.0" text="ID SERVICO" />
            <TableColumn fx:id="colunaDescricao" prefWidth="75.0" text="DESCRICAO" />
          <TableColumn fx:id="colunaPreco" prefWidth="75.0" text="PRECO" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="TOP_RIGHT" prefHeight="0.0" prefWidth="990.0" spacing="30.0" VBox.vgrow="NEVER">
         <children>
            <Label text="VALOR TOTAL:" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="System Bold Italic" size="21.0" />
               </font>
            </Label>
            <Label fx:id="lblPrecoTotal" text="R$0,00" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="System Bold Italic" size="21.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <ButtonBar nodeOrientation="LEFT_TO_RIGHT" prefHeight="101.0" prefWidth="541.0">
         <buttons>
            <Button fx:id="btnVoltar" mnemonicParsing="false" prefHeight="40.0" prefWidth="93.0" text="Voltar" />
         </buttons>
      </ButtonBar>
   </children>
</VBox>
