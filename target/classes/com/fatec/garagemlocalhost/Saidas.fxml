<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="520.0" prefWidth="824.0" spacing="20.0" styleClass="container" stylesheets="@style/containers-style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fatec.garagemlocalhost.SaidasController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <TableView fx:id="tabelaSaida" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="colunaIdSaida" prefWidth="75.0" text="ID SAÍDA" />
            <TableColumn fx:id="colunaIdPedido" prefWidth="75.0" text="ID PEDIDO" />
          <TableColumn fx:id="colunaAuxiliar" prefWidth="75.0" text="AUXILIAR" />
            <TableColumn fx:id="colunaPlaca" prefWidth="75.0" text="PLACA" />
            <TableColumn fx:id="colunaModelo" prefWidth="75.0" text="MODELO" />
            <TableColumn fx:id="colunaAno" prefWidth="75.0" text="ANO" />
            <TableColumn fx:id="colunaKmSaida" prefWidth="75.0" text="KM SAÍDA" />
            <TableColumn fx:id="colunaInstante" prefWidth="75.0" text="INSTANTE" />
            <TableColumn fx:id="colunaStatus" prefWidth="75.0" text="STATUS" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </TableView>
   </children>
</VBox>
